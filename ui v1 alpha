game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["FullBright"].Toggle.MouseButton1Down:Connect(function()
	settings.fullbright = not settings.fullbright;
	fullbright();
end)
	
game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Chams.Toggle.MouseButton1Down:Connect(function()
	settings.CHAMSEnabled = not settings.CHAMSEnabled
	if settings.CHAMSEnabled then
		game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Chams.Toggle.Text = "ON"
		game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Chams.Toggle.TextColor3 = settings.on
		warn("chams enabled")
		for _, v in next, Plrs:GetPlayers() do
			if v ~= MyPlr then
				if settings.CharAddedEvent[v.Name] == nil then
					settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
						if settings.CHAMSEnabled then
							RemoveChams(v)
							CreateChams(v)
						end
						repeat wait() until Char:FindFirstChild("HumanoidRootPart")
						TracerMT[v.Name] = Char.HumanoidRootPart
					end)
				end
				RemoveChams(v)
				CreateChams(v)
			end
		end
	else
		game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Chams.Toggle.Text = "OFF"
		game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Chams.Toggle.TextColor3 = settings.off
		warn("chams disabled")
		PlayerChams:ClearAllChildren()
		ItemChams:ClearAllChildren()
	end
end)
	
game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Tracers.Toggle.MouseButton1Down:Connect(function()
  settings.TracersEnabled = not settings.TracersEnabled
	if settings.TracersEnabled then
		warn("tracers enabled")
	game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Tracers.Toggle.Text = "ON"
	game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Tracers.Toggle.TextColor3= settings.on
	    for _, v in next, Plrs:GetPlayers() do
	      if v ~= MyPlr then
	        if settings.CharAddedEvent[v.Name] == nil then
	          settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
	            if settings.TracersEnabled then
	              RemoveTracers(v)
	              CreateTracers(v)
	            end
	          end)
	        end
	        if v.Character ~= nil then
	          local Tor = v.Character:FindFirstChild("HumanoidRootPart")
	          if Tor then
	            TracerMT[v.Name] = Tor
	          end
	        end
	        RemoveTracers(v)
	        CreateTracers(v)
	      end
	    end
	else
	warn("tracers disabled")
	game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Tracers.Toggle.Text = "OFF"
	game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.Tracers.Toggle.TextColor3= settings.off
	    for _, v in next, Plrs:GetPlayers() do
	      RemoveTracers(v)
	    end
	  end
end)
	
game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.ESP.Toggle.MouseButton1Down:Connect(function()
		settings.ESPEnabled = not settings.ESPEnabled
		if settings.ESPEnabled then
			warn("esp enabled")
			game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.ESP.Toggle.Text = "ON"
			game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.ESP.Toggle.TextColor3 = settings.on
			for _, v in next, Plrs:GetPlayers() do
				if v ~= MyPlr then
					if settings.CharAddedEvent[v.Name] == nil then
						settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
							if settings.ESPEnabled then
								RemoveESP(v)
								CreateESP(v)
							end
							repeat wait() until Char:FindFirstChild("HumanoidRootPart")
							TracerMT[v.Name] = Char.HumanoidRootPart
						end)
					end
					RemoveESP(v)
					CreateESP(v)
				end
			end
		else
			warn("esp disabled")
			game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.ESP.Toggle.Text = "OFF"
			game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Visuals").Selection["ESP Options"].Options.ESP.Toggle.TextColor3 = settings.off
			PlayerESP:ClearAllChildren()
			ItemESP:ClearAllChildren()
		end
end)

