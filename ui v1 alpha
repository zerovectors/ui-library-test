-- miscellaneous shit
game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Low Gravity"].Toggle.MouseButton1Down:Connect(function()
    settings.gravity = not settings.gravity;
    if settings.gravity then
        warn("low gravity enabled");
        game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Low Gravity"].Toggle.Text = "ON";
        game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Low Gravity"].Toggle.TextColor3 = settings.on
        workspace.Gravity = 50;
    else
        warn("low gravity disabled");
        game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Low Gravity"].Toggle.Text = "OFF";
        game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Low Gravity"].Toggle.TextColor3 = settings.off
        workspace.Gravity = 196;
    end;
end)

local maxval = math.floor(game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Scroller.CanvasPosition.X / (game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Scroller.CanvasSize.X.Offset - game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Scroller.AbsoluteWindowSize.X) * 196)
game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Value.Text = tostring(maxval * 1);
game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Value2.Text = "["..tostring(maxval * 1).."/196]";
game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Scroller.Changed:Connect(function(c) if (c ~= "CanvasPosition") then return end
    local maxval = math.floor(game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Scroller.CanvasPosition.X / (game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Scroller.CanvasSize.X.Offset - game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Scroller.AbsoluteWindowSize.X) * 196)
    game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Value.Text = tostring(maxval * 1);
    game.Players.LocalPlayer.PlayerGui["Vect0rs hub"].mainframe:WaitForChild("Miscellaneous").Selection["Gravity Slider"].Value2.Text = "["..tostring(maxval * 1).."/196]";
    game.workspace.Gravity = maxval * 1
end)
end;


Plrs.PlayerAdded:connect(function(Plr)
	if settings.CharAddedEvent[Plr.Name] == nil then
		settings.CharAddedEvent[Plr.Name] = Plr.CharacterAdded:connect(function(Char)
			if settings.TracersEnabled then
				CreateTracers(Plr)
			end
			if settings.CHAMSEnabled then
				CreateChams(Plr)
			end
			if settings.ESPEnabled then
				RemoveESP(Plr)
				CreateESP(Plr)
			end
			repeat wait() until Char:FindFirstChild("HumanoidRootPart")
			TracerMT[Plr.Name] = Char.HumanoidRootPart
		end)
	end
end)

Plrs.PlayerRemoving:connect(function(Plr)
	--ChatLog(Plr)
	if settings.CharAddedEvent[Plr.Name] ~= nil then
		settings.CharAddedEvent[Plr.Name]:Disconnect()
		settings.CharAddedEvent[Plr.Name] = nil
	end
	RemoveTracers(Plr)
	RemoveChams(Plr)
	RemoveESP(Plr)
	TracerMT[Plr.Name] = nil
end)

-- loader --
createtemplates();
colourshit();
createuioptions();
mousebuttonscriptshit();
