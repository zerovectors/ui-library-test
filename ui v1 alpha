	buttontemplatebutton.Position = UDim2.new(0.838, 0,0, 0);
	buttontemplatebutton.Size = UDim2.new(0, 44,0, 30);
	buttontemplatebutton.TextColor3 = Color3.fromRGB(255,255,255);
